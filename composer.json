{
    "name": "brainappeal/t3monitor",
    "type": "typo3-cms-extension",
    "description": "Monitors TYPO3 installation for updates and security issues (Requires an account on https://www.t3monitor.de)",
    "keywords": [
        "TYPO3",
        "extension",
        "t3monitor"
    ],
    "homepage": "https://www.t3monitor.de",
    "authors": [{
        "name": "Brain Appeal GmbH",
        "role": "Developer",
        "homepage": "https://www.brainappeal.de"
    }],
    "license": [
        "GPL-2.0+"
    ],
    "require": {
        "typo3/cms-core": ">=7.6.0,<8.0"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/Web"
        }
    }
}
